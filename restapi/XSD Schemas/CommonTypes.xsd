<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified"
           id="CommonTypes">
    <xs:complexType name="AddInfo">
        <xs:annotation>
            <xs:documentation>Дополнительная информация</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="AddField" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Поле с дополнительной информацией</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="xs:string">
                            <xs:attribute name="Name" type="xs:string" use="required">
                                <xs:annotation>
                                    <xs:documentation>Наименование поля</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ArbitrationSummary">
        <xs:annotation>
            <xs:documentation>Сводные данные об арбитражных делах</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Plaintiff" type="ArbitrationSummaryByRole">
                <xs:annotation>
                    <xs:documentation>Данные о делах, в которых компания выступает в качестве истца</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Defendant" type="ArbitrationSummaryByRole">
                <xs:annotation>
                    <xs:documentation>Данные о делах, в которых компания выступает в качестве ответчика
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ArbitrationByType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Сводные данные об арбитражных делах по типам</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Type" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Тип арбитражных дел</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="Plaintiff" type="ArbitrationSummaryByRoleShort">
                                        <xs:annotation>
                                            <xs:documentation>Данные о делах, в которых компания выступает в качестве
                                                истца
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="Defendant" type="ArbitrationSummaryByRoleShort">
                                        <xs:annotation>
                                            <xs:documentation>Данные о делах, в которых компания выступает в качестве
                                                ответчика
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                </xs:sequence>
                                <xs:attribute name="Id" type="xs:int" use="required">
                                    <xs:annotation>
                                        <xs:documentation>Идентификатор типа</xs:documentation>
                                    </xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="Name" type="xs:string" use="required">
                                    <xs:annotation>
                                        <xs:documentation>Наименование типа</xs:documentation>
                                    </xs:annotation>
                                </xs:attribute>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="ArbitrationByYear">
                <xs:annotation>
                    <xs:documentation>Сводные данные об арбитражных делах по годам</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Year" type="ArbitrationSummaryByYear" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Сводные данные об арбитражных делах за год</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="Total" type="xs:int" use="required">
            <xs:annotation>
                <xs:documentation>Общее количество дел</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Considered" type="xs:int" use="required">
            <xs:annotation>
                <xs:documentation>Количество рассматриваемых дел</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Appealed" type="xs:int" use="required">
            <xs:annotation>
                <xs:documentation>Количество обжалуемых дел</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="DecisionsAndRulings" type="xs:int" use="required">
            <xs:annotation>
                <xs:documentation>Количество решений и постановлений</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Completed" type="xs:int" use="required">
            <xs:annotation>
                <xs:documentation>Количество завершенных дел</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="ArbitrationSummaryShort">
        <xs:annotation>
            <xs:documentation>Сводные данные об арбитражных делах</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Year" type="ArbitrationSummaryByYear" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Сводные данные об арбитражных делах за год</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="Total" type="xs:int" use="required">
            <xs:annotation>
                <xs:documentation>Общее количество дел</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Considered" type="xs:int" use="required">
            <xs:annotation>
                <xs:documentation>Количество рассматриваемых дел</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Appealed" type="xs:int" use="required">
            <xs:annotation>
                <xs:documentation>Количество обжалуемых дел</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="DecisionsAndRulings" type="xs:int" use="required">
            <xs:annotation>
                <xs:documentation>Количество решений и постановлений</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Completed" type="xs:int" use="required">
            <xs:annotation>
                <xs:documentation>Количество завершенных дел</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="ArbitrationSummaryByRole">
        <xs:annotation>
            <xs:documentation>Сводные данные об арбитражных делах для отдельной роли участника</xs:documentation>
        </xs:annotation>
        <xs:attribute name="Total" type="xs:int" use="required">
            <xs:annotation>
                <xs:documentation>Общее количество дел</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="TotalSum" type="MyDecimal" use="required">
            <xs:annotation>
                <xs:documentation>Сумма по всем делам</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Considered" type="xs:int" use="required">
            <xs:annotation>
                <xs:documentation>Количество рассматриваемых дел</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ConsideredSum" type="MyDecimal" use="required">
            <xs:annotation>
                <xs:documentation>Сумма по рассматриваемым делам</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Appealed" type="xs:int" use="required">
            <xs:annotation>
                <xs:documentation>Количество обжалуемых дел</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="AppealedSum" type="MyDecimal" use="required">
            <xs:annotation>
                <xs:documentation>Сумма по обжалуемым делам</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="DecisionsAndRulings" type="xs:int" use="required">
            <xs:annotation>
                <xs:documentation>Количество решений и постановлений</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="DecisionsAndRulingsSum" type="MyDecimal" use="required">
            <xs:annotation>
                <xs:documentation>Сумма по решениям и постановлениям</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Completed" type="xs:int" use="required">
            <xs:annotation>
                <xs:documentation>Количество завершенных дел</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="CompletedSum" type="MyDecimal" use="required">
            <xs:annotation>
                <xs:documentation>Сумма по завершенным делам</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="ArbitrationSummaryByRoleShort">
        <xs:annotation>
            <xs:documentation>Сводные данные об арбитражных делах для отдельной роли участника</xs:documentation>
        </xs:annotation>
        <xs:attribute name="Total" type="xs:int" use="required">
            <xs:annotation>
                <xs:documentation>Общее количество дел</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Considered" type="xs:int" use="required">
            <xs:annotation>
                <xs:documentation>Количество рассматриваемых дел</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Appealed" type="xs:int" use="required">
            <xs:annotation>
                <xs:documentation>Количество обжалуемых дел</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Completed" type="xs:int" use="required">
            <xs:annotation>
                <xs:documentation>Количество завершенных дел</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="ArbitrationSummaryByYear">
        <xs:annotation>
            <xs:documentation>Сводные данные об арбитражных делах за год</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Plaintiff">
                <xs:annotation>
                    <xs:documentation>Данные о делах, в которых компания/ИП выступает в качестве истца
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="CasesNumber" type="xs:int" use="required">
                        <xs:annotation>
                            <xs:documentation>Количество дел</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Sum" type="MyDecimal" use="required">
                        <xs:annotation>
                            <xs:documentation>Сумма</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
            <xs:element name="Defendant">
                <xs:annotation>
                    <xs:documentation>Данные о делах, в которых компания/ИП выступает в качестве ответчика
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="CasesNumber" type="xs:int" use="required">
                        <xs:annotation>
                            <xs:documentation>Количество дел</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Sum" type="MyDecimal" use="required">
                        <xs:annotation>
                            <xs:documentation>Сумма</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
            <xs:element name="ThirdOrOtherPerson">
                <xs:annotation>
                    <xs:documentation>Данные о делах, в которых компания/ИП выступает в качестве третьего лица
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="CasesNumber" type="xs:int" use="required">
                        <xs:annotation>
                            <xs:documentation>Количество дел</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="Year" use="required">
            <xs:annotation>
                <xs:documentation>Год</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="BankruptcyMessage">
        <xs:complexContent>
            <xs:extension base="Message">
                <xs:attribute name="CaseNumber" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Номер дела</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="Category">
        <xs:annotation>
            <xs:documentation>Категория</xs:documentation>
        </xs:annotation>
        <xs:attribute name="Code" type="xs:int" use="required">
            <xs:annotation>
                <xs:documentation>Код категории</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Name" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>Наименование категории</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="ChangeInNameAndLegalFormType">
        <xs:all>
            <xs:element name="Name" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Наименование</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="OGRN" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Код ОГРН</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="INN" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Код ИНН</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="OKOPF" type="OKOPF" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Организационно-правовая форма</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ChangeDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата изменений</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:all>
    </xs:complexType>
    <xs:complexType name="CompanyByPersonType">
        <xs:annotation>
            <xs:documentation>Компания</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Company">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="SPARKID" type="SparkID">
                            <xs:annotation>
                                <xs:documentation>Идентификатор компании в СПАРК</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="INN" type="xs:string" minOccurs="0"/>
                        <xs:element name="OGRN" type="xs:string" minOccurs="0"/>
                        <xs:element name="OKPO" type="xs:string" minOccurs="0"/>
                        <xs:element name="FullName" type="xs:string"/>
                        <xs:element name="RegionCode" type="xs:string" minOccurs="0"/>
                        <xs:element name="IsActing" type="xs:boolean"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="Person">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="FIO" type="xs:string"/>
                        <xs:element name="INN" type="xs:string" minOccurs="0"/>
                        <xs:element name="Position" minOccurs="0">
                            <xs:complexType>
                                <xs:attribute name="Code" type="xs:int" use="required"/>
                                <xs:attribute name="Name" type="xs:string" use="required"/>
                                <xs:attribute name="IsActing" type="xs:boolean" use="required"/>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="IsOwner" type="xs:boolean"/>
                        <xs:element name="IsActing" type="xs:boolean" minOccurs="0"/>
                        <xs:element name="SharePart" type="MyDecimal" minOccurs="0"/>
                        <xs:element name="SharePartRUR" type="MyDecimal" minOccurs="0"/>
                        <xs:element name="ActualDate" type="xs:date" minOccurs="0"/>
                        <xs:element name="DataSource" type="xs:string"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CompanyForCompliance">
        <xs:annotation>
            <xs:documentation>Российская компания для X-Compliance</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="SparkID" type="SparkID">
                <xs:annotation>
                    <xs:documentation>Идентификатор в СПАРК</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="INN" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>ИНН</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="OGRN" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>ОГРН</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FullName" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Полное наименование</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Address" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Адрес местонахождения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RegionCode" type="xs:int" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Код региона</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RegionName" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Наименование региона</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CompaniesNumberWithSameInfo">
        <xs:annotation>
            <xs:documentation>Количество компаний с аналогичными данными</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="TelephoneCount">
                <xs:annotation>
                    <xs:documentation>Количество компаний с аналогичным телефоном</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="xs:int">
                            <xs:attribute name="PhoneCode" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation>Региональный код</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="PhoneNumber" type="xs:string">
                                <xs:annotation>
                                    <xs:documentation>Номер телефона</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="PhoneList" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Количество компаний с аналогичными телефонами</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="PhoneCount" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Количество компаний с аналогичным телефоном</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:simpleContent>
                                    <xs:extension base="xs:int">
                                        <xs:attribute name="Code" type="xs:string">
                                            <xs:annotation>
                                                <xs:documentation>Региональный код</xs:documentation>
                                            </xs:annotation>
                                        </xs:attribute>
                                        <xs:attribute name="Number" type="xs:string">
                                            <xs:annotation>
                                                <xs:documentation>Номер телефона</xs:documentation>
                                            </xs:annotation>
                                        </xs:attribute>
                                    </xs:extension>
                                </xs:simpleContent>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="AddressCount" type="xs:int">
                <xs:annotation>
                    <xs:documentation>Количество компаний с аналогичным адресом</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AddressWithoutRoomCount" type="xs:int">
                <xs:annotation>
                    <xs:documentation>Количество компаний с аналогичным адресом с точностью до здания</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AddressNotAffiliatedCount" type="xs:int">
                <xs:annotation>
                    <xs:documentation>Количество не аффилированных компаний с аналогичным адресом</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AddressFTSCount" type="xs:int">
                <xs:annotation>
                    <xs:documentation>Количество компаний с аналогичным адресом по данным ФНС</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ManagerCountInCountry" type="xs:int">
                <xs:annotation>
                    <xs:documentation>Количество компаний с аналогичным руководителем</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ManagerCountInRegion" type="xs:int">
                <xs:annotation>
                    <xs:documentation>Количество компаний с аналогичным руководителем, зарегистрированных в том же
                        регионе
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ManagerInnCount" type="xs:int">
                <xs:annotation>
                    <xs:documentation>Количество компаний с аналогичным ИНН руководителя</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CompanySize">
        <xs:annotation>
            <xs:documentation>Размер компании</xs:documentation>
        </xs:annotation>
        <xs:attribute name="Revenue" type="xs:string">
            <xs:annotation>
                <xs:documentation>Значение оценки доходов компании</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Description" type="xs:string">
            <xs:annotation>
                <xs:documentation>Описание значения оценки доходов компании</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="CompanyType">
        <xs:annotation>
            <xs:documentation>Компания</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Name" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Наименование компании</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SPARKID" type="SparkID" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Идентификатор компании в СПАРК</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Address" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Адрес компании</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="INN" type="xs:string" minOccurs="0"/>
            <xs:element name="OGRN" type="xs:string" minOccurs="0"/>
            <xs:element name="OKPO" type="xs:string" minOccurs="0"/>
            <xs:element name="OKOPFName" type="xs:string" minOccurs="0"/>
            <xs:element name="Manager" type="xs:string" minOccurs="0"/>
