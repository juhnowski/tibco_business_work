    <xs:simpleType name="Resident">
        <xs:annotation>
            <xs:documentation>Y – резидент, N-нерезидент</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Y"/>
            <xs:enumeration value="N"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SystemType">
        <xs:annotation>
            <xs:documentation>Идентификатор инициирующей системы.
                Возможные значение:
                • USSS
                • MWP
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="40"/>
            <xs:enumeration value="USSS"/>
            <xs:enumeration value="MWP"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="BillingType">
        <xs:annotation>
            <xs:documentation>Признак, к какому биллингу относится подключаемая услуга.
                Допустимы следующие варианты: Mobile, Fix, Convergent
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Mobile"/>
            <xs:enumeration value="Fix"/>
            <xs:enumeration value="Convergent"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="BenSelectionType">
        <xs:annotation>
            <xs:documentation>D-default ben, E-существующий, N-новый
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="D"/>
            <xs:enumeration value="E"/>
            <xs:enumeration value="N"/>
        </xs:restriction>
    </xs:simpleType>
</xs:schema>
