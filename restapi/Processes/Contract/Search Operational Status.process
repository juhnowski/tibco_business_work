        <pd:resourceType>ae.activities.JDBCCallActivity</pd:resourceType>
        <pd:x>224</pd:x>
        <pd:y>58</pd:y>
        <config>
            <timeout>%%Timeout%%</timeout>
            <maxRows>1000000</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/Connection/MSSQL Connection (SF).sharedjdbc</jdbcSharedConfig>
            <ProcedureName>OperationalStatus_GetList;1</ProcedureName>
            <useSchema>false</useSchema>
            <commit>false</commit>
            <schema>%%Schema%%</schema>
            <parameterTypes>
                <parameter>
                    <colName>@RETURN_VALUE</colName>
                    <colType>5</colType>
                    <dataType>4</dataType>
                    <typeName>int</typeName>
                </parameter>
                <parameter>
                    <colName>@mainSearch</colName>
                    <colType>1</colType>
                    <dataType>12</dataType>
                    <typeName>varchar</typeName>
                </parameter>
                <parameter>
                    <colName>@pageSize</colName>
                    <colType>1</colType>
                    <dataType>4</dataType>
                    <typeName>int</typeName>
                </parameter>
                <parameter>
                    <colName>@currentPage</colName>
                    <colType>1</colType>
                    <dataType>4</dataType>
